        <Canvas x:Name="field">
            <Line X1="50" X2="50" Y1="30" Y2="430" Stroke="Black"/>
            <Line X1="50" X2="44" Y1="30" Y2="40" Stroke="Black"/>
            <Line X1="50" X2="56" Y1="30" Y2="40" Stroke="Black"/>
            <Line X1="50" X2="640" Y1="430" Y2="430" Stroke="Black"/>
            <Line X1="630" X2="640" Y1="424" Y2="430" Stroke="Black"/>
            <Line X1="630" X2="640" Y1="436" Y2="430" Stroke="Black"/>
            <TextBlock Text="Книги" Canvas.Top="40" Canvas.Left="11"/>
